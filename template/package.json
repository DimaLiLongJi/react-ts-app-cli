{
  "name": "cf-tutor",
  "description": "careerfrog 微信导师端项目",
  "version": "0.1.0",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-async-to-generator"
    ]
  },
  "scripts": {
    "start": "rm -rf web/scripts && nodemon app.js & webpack --watch --mode=development",
    "lint": "eslint $(find . \\( -name \"*.js\" \\) -not \\( -path \"./coverage/*\" -o -path \"./node_modules/*\" -o -path \"./web/*\" -o -path \"./build/*\" -o -path \"./dist/*\" -o -path \"./rev/*\" -o -path \"./webpack.config.js\" -o -path \"./gulpfile.js\" -o -name \".*\" \\))",
    "test": "NODE_ENV=mocha nyc _mocha ./test/ -R spec --recursive -u tdd --timeout 999999 --colors --exit",
    "dev-test": "NODE_ENV=dev-mocha nyc _mocha ./test/ -R spec --recursive -u tdd --timeout 999999 --colors --exit",
    "build-webpack": "webpack --env=production --mode=production --progress",
    "build-gulp": "gulp clean && gulp dist && gulp build && gulp rev && gulp pub-static",
    "pub-local-test": "rm -rf web/scripts && npm run build-webpack && npm run build-gulp && nodemon app.js",
    "admin": "NODE_ENV=mocha _mocha ./test/ -R spec --recursive -u tdd --timeout 999999 --colors --exit --grep 'admin test'"
  },
  "nyc": {
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "keywords": [],
  "author": "",
  "dependencies": {
    "antd": "^3.5.3",
    "antd-mobile": "^2.1.9",
    "async-redis": "^1.1.4",
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "body-parser-xml": "^1.1.0",
    "classnames": "^2.2.5",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "express-validation": "^1.0.2",
    "joi": "^13.3.0",
    "js-cookie": "^2.2.0",
    "loadable-components": "^0.4.0",
    "lodash": "^4.17.10",
    "log4js": "2.3.3",
    "moment": "^2.14.1",
    "mongodb": "^3.0.8",
    "pinyin": "^2.8.3",
    "prop-types": "^15.6.0",
    "qs": "^6.4.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-medium-image-zoom": "^3.0.12",
    "react-redux": "^5.0.6",
    "react-render-html": "^0.6.0",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "react-scroll": "^1.7.4",
    "redux": "^4.0.0",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "superagent": "^3.8.3",
    "wechat-api": "^1.35.1",
    "window-or-global": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-async-to-generator": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@types/classnames": "^2.2.4",
    "@types/prop-types": "^15.5.3",
    "@types/qs": "^6.5.1",
    "@types/react": "^16.7.22",
    "@types/react-dom": "^16.0.11",
    "@types/react-helmet": "^5.0.6",
    "@types/react-redux": "^6.0.4",
    "@types/react-router": "^4.0.28",
    "@types/react-router-config": "^1.0.7",
    "@types/react-router-dom": "^4.2.7",
    "@types/window-or-global": "^1.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^8.2.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-dynamic-import-webpack": "^1.0.2",
    "babel-plugin-import": "^1.7.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.0",
    "eslint-config-airbnb-base": "^11.1.1",
